  ((and (cons? V5313) (cons? (hd V5313)))
   (do (shen-remtype (hd (hd V5313))) (shen-unwind-types V5312 (tl V5313))))
  (true (shen-sys-error shen-unwind-types))))

(defun shen-remtype (V5314)
