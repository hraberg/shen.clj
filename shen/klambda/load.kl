(defun load (V5288)
 (let Load
  (let Start (get-time run)
   (let Result (shen-load-help (value shen-*tc*) (read-file V5288))
    (let Finish (get-time run)
     (let Time (- Finish Start)
      (let Message (intoutput "~%run time: ~A secs~%" (@p Time ()))
       Result)))))
  (let Infs
   (if (value shen-*tc*)
    (intoutput "~%typechecked in ~A inferences~%" (@p (inferences _) ()))
    shen-skip)
   loaded)))

(defun shen-load-help (V5293 V5294)
 (cond
  ((= false V5293)
   (map (lambda X (intoutput "~S~%" (@p (shen-eval-without-macros X) ())))
    V5294))
  (true
   (let RemoveSynonyms
    (mapcan (lambda V5295 (shen-remove-synonyms V5295)) V5294)
    (let Table (mapcan (lambda V5296 (shen-typetable V5296)) RemoveSynonyms)
     (let Assume (map (lambda V5297 (shen-assumetype V5297)) Table)
      (trap-error
       (map (lambda V5298 (shen-typecheck-and-load V5298)) RemoveSynonyms)
       (lambda E (shen-unwind-types E Table)))))))))

(defun shen-remove-synonyms (V5299)
