  ((and (cons? V5305) (and (= define (hd V5305)) (cons? (tl V5305))))
   (let Sig
    (compile (lambda V5306 (shen-<sig+rest> V5306)) (tl (tl V5305)) ())
    (if (= Sig fail!)
     (interror "~A lacks a proper signature.~%" (@p (hd (tl V5305)) ()))
     (cons (cons (hd (tl V5305)) Sig) ()))))
  (true ())))

(defun shen-assumetype (V5307)
